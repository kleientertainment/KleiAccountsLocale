#. i18n: keyword: RewardLinkResultTitle
msgid "Reward Link"
msgstr "Liên kết tới phần thường"

#. i18n: keyword: RewardLinkResultDescription
msgid "Congratulations!</p><p>You have found a valid reward link in the wild."
msgstr "Xin chúc mừng!</p><p>Bạn đã tìm thấy một liên kết tới phần thường hợp lệ từ bên ngoài."

#. i18n: keyword: RewardLinkResultMessagePoints
msgid "You have claimed {{.Amount}} {{.Name}}."
msgstr "Bạn đã lấy được {{.Amount}} {{.Name}}."

#. i18n: keyword: RewardLinkResultMessageSpool
